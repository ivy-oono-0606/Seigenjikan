<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Command4Fragment">

    <ImageView
        android:id="@+id/com4.1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/button"
        app:layout_constraintEnd_toStartOf="@+id/com4.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/com4.2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/button"
        app:layout_constraintEnd_toStartOf="@+id/com4.3"
        app:layout_constraintStart_toEndOf="@+id/com4.1"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/com4.3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/button"
        app:layout_constraintEnd_toStartOf="@+id/com4.4"
        app:layout_constraintStart_toEndOf="@+id/com4.2"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/com4.4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/com4.3"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>